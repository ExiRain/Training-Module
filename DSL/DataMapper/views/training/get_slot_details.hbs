{
    "name":"{{ name }}",
    "type":"{{ hits.type}}",
    "influenceConversation": {{ eq true hits.influence_conversation }},
    "mappings": {
        "type": "{{ hits.mappings.type }}",
        "entity": "{{ hits.mappings.entity }}",
        "intent": [{{#each hits.mappings.intent}}"{{ this }}"{{#unless @last}},{{/unless}}{{/each}}],
        "notIntent": [{{#each hits.mappings.not_intent}}"{{ this }}"{{#unless @last}},{{/unless}}{{/each}}]
    }
}